<h1>Listing sample_dates</h1>

<table>
  <thead>
    <tr>
      <th>Sample date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sample_dates.each do |sample_date| %>
      <tr>
        <td><%= sample_date.sample_date %></td>
        <td><%= link_to 'Show', sample_date %></td>
        <td><%= link_to 'Edit', edit_sample_date_path(sample_date) %></td>
        <td><%= link_to 'Destroy', sample_date, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sample date', new_sample_date_path %>
